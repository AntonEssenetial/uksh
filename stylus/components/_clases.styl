// Socials sprites 

.footer__soc1
  margin 5px 0 0 0
  a
    &:first-child
      sprite($sprite-soc1)
      &:hover 
        sprite($sprite-soc1a)
    &:nth-child(2)
      sprite($sprite-soc2)
      &:hover 
        sprite($sprite-soc2a)
    &:nth-child(3)
      sprite($sprite-soc3)
      &:hover 
        sprite($sprite-soc3a)
    &:last-child
      sprite($sprite-soc4)
      &:hover 
        sprite($sprite-soc4a)
.footer__soc2
  a
    &:first-child
      sprite($sprite-soc5)
      &:hover 
        sprite($sprite-soc5a)
    &:nth-child(2)
      sprite($sprite-soc6)
      &:hover 
        sprite($sprite-soc6a)
    &:nth-child(3)
      sprite($sprite-soc7)
      &:hover 
        sprite($sprite-soc7a)
    &:last-child
      sprite($sprite-soc8)
      &:hover 
        sprite($sprite-soc8a)
        
