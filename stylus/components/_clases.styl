// bx-lider-2
.bx-slider-2-wrapper
  background: rgba(39, 52, 128, 0.88)
  width 466px 
  padding 17px 25px
  overflow hidden
  border-right 6px solid #e2ebed
  box-shadow: 0 0 7px rgba(0, 0, 0, 0.38)
  p 
    color white !important
    medium()
    font-size 16px
    line-height 25px !important
#next 
  border-bottom 1px solid #959789 
  margin-top 7px
#next
#prev
  background: #fbfcfd
  width 30px 
  height 35px
  cursor: pointer
  box-shadow: 1px 0 2px 0px rgba(0, 0, 0, 0.39);
.bx-controls
  right 0px
  top 0px
.bx-contros-slider
  position absolute
  top 80px
  z-index 1
  right 100px
#next 
  cursor pointer
  display flex
  .icon 
    margin auto auto
    sprite($sprite-left5)
    display block
  &:hover 
    .icon
      sprite($sprite-left5a)
#prev 
  cursor pointer
  display flex
  .icon 
    margin auto auto
    sprite($sprite-right2)
    display block
  &:hover 
    .icon
      sprite($sprite-right2a)
      

// Section border 

.img-border-wrapper
