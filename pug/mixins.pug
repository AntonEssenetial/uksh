mixin header()
  header.header
    .header__top
      .container-wrapper
        .row.end
          .header__col
            a.str21.bold.icon-1.row(href="tel:+73433790069") +7 (343) 379-00-69
            a.str21.bold.icon-2.row(href="tel:+73433712700") +7 (343) 371-27-00
            a.str10.bold.icon-3.row(href="mailto:info@uksh.ru") info@uksh.ru
          .header__col
            .row
              .icon-4
              .wrapper
