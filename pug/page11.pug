extends layout
include mixins.pug

block content
  +header()
  section.section__content
    .container-wrapper
      +bread-crumbs([
        "Главная", 
        "машиностроение"
      ])
      +heading([
        'машиностроение'
      ])
      .container.container_top.container_bot 
        .row.filter__items.between
          ul
            li
              span.btn.filter(data-filter=".category-1") МАШИНОСТРОЕНИЕ
            li
              span.btn.filter(data-filter=".category-2") ГОРНОДОБЫВАЮЩАЯ ПРОМЫШЛЕННОСТЬ
            li
              span.btn.filter(data-filter='.category-3') СТРОИТЕЛЬНАЯ ИНДУСТРИЯ    
            li
              span.btn.filter(data-filter='.category-4') ПЕРЕДИСЛОКАЦИЯ ОБОРУДОВАНИЯ 
            li
              span.btn.filter(data-filter='.category-5') ПРОЕКТИРОВАНИЕ 
          ul
            li
              span.btn.filter(data-filter='.category-6') ЧЕРНАЯ МЕТАЛЛУРГИЯ И ТРУБНАЯ ПРОМЫШЛЕННОСТ 
            li
              span.btn.filter(data-filter='.category-7') ХИМИЧЕСКАЯ И НЕФТЕГАЗОВАЯ ПРОМЫШЛЕННОСТЬ
            li
              span.btn.filter(data-filter='.category-8') ОЧИСТНЫЕ СООРУЖЕНИЯ И ГАЗООТЧИСТКА
            li
              span.btn.filter(data-filter='.category-9') ГОСУДАРСТВЕННЫЕ И МУНИЦИПАЛЬНЫЕ ЗАКАЗЧИКИ
            li
              span.btn.filter(data-filter='.category-10') ЭЛЕКТРОМОНТАЖНЫЕ РАБОТЫ 
          ul
            li
              span.btn.filter(data-filter='.category-11') ЦВЕТНАЯ МЕТАЛЛУРГИЯ
            li
              span.btn.filter(data-filter='.category-12') ЭНЕРГЕТИКА
            li
              span.btn.filter(data-filter='.category-13') АГРОПРОМЫШЛЕННОСТЬ
            li
              span.btn.filter(data-filter='.category-14') ЛОГИСТИЧЕСКИЕ ОБЪЕКТЫ
            li
              span.btn.filter(data-filter="all") все
